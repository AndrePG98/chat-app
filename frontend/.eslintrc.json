{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    // "eslint:recommended",
    "next",
    // "plugin:@typescript-eslint/eslint-recommended",
    "prettier"
    // "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react",
    "react-hooks"
  ],
  "rules": {
    "react/prop-types": 0
  }
}